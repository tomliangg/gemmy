{
  "name": "gemini-chat",
  "displayName": "gemini-chat",
  "description": "chatbot with gemini",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gemini-chat.tada",
        "title": "tada"
      },
      {
        "command": "gemini-chat.webview",
        "title": "show webview"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:watch": "tsc -b && vite build --watch",
    "compile": "tsc -p tsconfig.vsc-extension.json",
    "watch": "tsc -watch -p tsconfig.vsc-extension.json",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.92.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "sass-embedded": "^1.77.8",
    "typescript": "^5.4.5",
    "vite": "^5.4.2"
  },
  "dependencies": {
    "@emotion/css": "^11.13.0",
    "@google/generative-ai": "^0.17.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "highlight.js": "^11.10.0",
    "lucide-react": "^0.437.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1",
    "rehype-highlight": "^7.0.0",
    "remark-gfm": "^4.0.0"
  }
}
